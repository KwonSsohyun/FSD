	Label la;
	
	public Ex07(){ // UI쪽 하나의 쓰레드
		la = new Label();
		
		setBounds(200,200,200,100);
		setVisible(true);
	}
	
	
	
	
	public static void main(String[] args) {
		// 메인쪽 하나의 쓰레드
		// 시계
		
		Ex07 thr = new Ex07();
		
		while(true){
			Date d = new Date();
		}

	}

}
