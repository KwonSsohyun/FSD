package com.hi.step03;

import java.net.*;
import java.io.*;

public class Server {

	public static void main(String[] args) {
		ServerSocket serve = null;
		
		
		
		try {
			serve = new ServerSocket(5000);
			
			
			
			
		} catch (IOException e) {
			e.printStackTrace();
		} finally {
			try {
				serve.close();
			} catch (IOException e) {
				e.printStackTrace();
			}
		}
		

	}

}
