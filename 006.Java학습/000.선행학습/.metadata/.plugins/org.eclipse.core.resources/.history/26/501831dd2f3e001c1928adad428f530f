import java.util.Scanner;

public class Quiz2 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.println("몇 행 으로 출력하시겠습니까 숫자만 입력해주세요 : ");
		int num = scan.nextInt();
				
		for(int i=num; i>=0; i--) { // 전체적 포문
			for (int x = 0 ; x <= i-1; x++) {
				System.out.print(' ');
			}
			System.out.println(' ');
		}			
	}
}
