import java.util.Scanner;

public class Quiz2 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.println("몇 행 : ");
		
		int num = scan.nextInt();
		
		for(int i = 1; i <= num ; i++) { // 바깥루프
			for(int j = num ; j == num ; j++) {
				System.out.print("*");
			}
			System.out.print();
		}
		
			

	}

}
