
public class Ex05구구단 {

	public static void main(String[] args) {
		for(int dan = 2; dan <= 9 ; dan++) {
			for(int su = 1; su <= 9 ; su++) {
				System.out.println(dan + "x" + su + "=" + (dan*su));
				if(su == 6) continue; // continue; break; 나 뒤에 코드가 없기 때문에 결과는 같다.
			}
		System.out.println();
		}

	}

}
