
public class 안녕 {
	public static void main(String args[]) {
		System.out.println("반갑습니다. 자바 세계~"); // 문자열 
		System.out.println(100); // 숫자열
		System.out.println(5 * 3); // 메모리에 5, 3 올라오고 -> cpu 연산/제어
		System.out.println(5 / 3); // 정수와 정수는 소숫점 안나오고 정수만 나온다.
		System.out.println(5 % 3); // 나눗셈의 나머지가 나온다.
	}
}
