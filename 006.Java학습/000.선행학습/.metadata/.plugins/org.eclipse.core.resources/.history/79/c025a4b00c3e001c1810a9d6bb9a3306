
public class Ex06구구단2 {

	public static void main(String[] args) {
		
		//Loop1 레이블 붙힘
		Loop1 : for(int dan = 2; dan <= 9 ; dan++) {
			for(int su = 1; su <= 9 ; su++) {
				
				System.out.println(dan + "x" + su + "=" + (dan*su));
				if(su == 5)break;
				}
				
			}

			System.out.println();
		}

	}

}
